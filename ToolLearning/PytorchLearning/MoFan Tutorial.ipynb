   "source": [
    "# Mofan Tutorial\n",
    "***\n",
    "\n",
    "> Pytorch 可以被称为 神经网络界的 Numpy"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Numpy = \n",
      " [[0 1 2]\n",
      " [3 4 5]] \n",
      " Torch = \n",
      " tensor([[0, 1, 2],\n",
      "        [3, 4, 5]]) \n",
      "\n"
     ]
    }
   ],
   "source": [
    "import torch\n",
    "import numpy as np\n",
    "\n",
    "np_data = np.arange(6).reshape(2,3)\n",
    "torch_data = torch.from_numpy(np_data)\n",
    "\n",
    "\n",
    "print(\"Numpy = \\n\", np_data,\"\\n\",\n",
    "      \"Torch = \\n\", torch_data,\"\\n\")"
   ]
  },
